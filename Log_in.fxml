      </Label>
      <Label layoutX="268.0" layoutY="185.0" text="Password">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="enterUsernameField" layoutX="343.0" layoutY="125.0" promptText="username" />
